<Window x:Class="Assignment5.HighScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment5"
        mc:Ignorable="d"
        Title="High Scores" Height="450" Width="400" ShowInTaskbar="False" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <ImageBrush ImageSource="Images/StarField.jpg" Stretch="UniformToFill"/>
    </Window.Background>
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0">
        <Label Content="High Scores" HorizontalAlignment="Center"/>
        <StackPanel Width="150">
            <Label Content="1. " />
            <Label Content="2. " />
            <Label Content="3. " />
            <Label Content="4. " />
            <Label Content="5. " />
            <Label Content="6. " />
            <Label Content="7. " />
            <Label Content="8. " />
            <Label Content="9. " />
            <Label Content="10. " />

        </StackPanel>
    </StackPanel>
</Window>
